<?xml version="1.0"?>
<!-- AIM 7 based on AIM-9 by xii Richard Harrison, 29-06-2015 -->
<PropertyList>
	<name>aim-7</name>
	<path>aim-7.ac</path>

	<animation>
		<type>select</type>
		<object-name>aim-7</object-name>
		<condition>
			<property>payload/armament/aim-7/flags/msl-id-700</property>
		</condition>
	</animation>

	<model>
		<path>Models/Stores/AIM-7/motor-smoke-7.xml</path>
		<offsets>
			<x-m>3</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</offsets>
	</model>

	<model>
		<path>Models/Stores/AIM-7/explosion-7.xml</path>
		<offsets>
			<x-m>3</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</offsets>
	</model>

	<animation>
        <object-name>aim-7</object-name>
        <enable-hot type="bool">false</enable-hot>
    </animation>

</PropertyList>
